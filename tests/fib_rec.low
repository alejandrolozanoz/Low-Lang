funcion int fibonacci(int o)
{
   si (o<0) entonces {
      escribe("Incorrect input");
      regresa ("Error");
   }
   si (o==1) entonces {
      regresa (0);
    }
   si (o==2) entonces {
      regresa (1);
   }
   regresa (fibonacci(o-1)+fibonacci(o-2));
}

